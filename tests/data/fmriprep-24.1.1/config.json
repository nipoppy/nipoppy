        "URI": "docker://nipreps/[[PIPELINE_NAME]]:[[PIPELINE_VERSION]]"
    },
    "CONTAINER_CONFIG": {
        "ENV_VARS": {
            "TEMPLATEFLOW_HOME": "[[TEMPLATEFLOW_HOME]]"
